{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.d.ts"
  ],
  "compilerOptions": {
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
    },
    "baseUrl": ".",
    "moduleResolution": "bundler"
  }
}
